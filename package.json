{
  "name": "pk-ordering",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:src:tests": "eslint \"{src,apps,libs,test}/**/*.spec.ts\" --fix-dry-run",
    "//": "jest --projects uses specified jest config file",
    "test": "WEBSOCKET_SERVER=6789 jest",
    "test:watch": "WEBSOCKET_SERVER=6789 jest --watch",
    "test:cov": "WEBSOCKET_SERVER=6789 jest --coverage",
    "test:unit": "jest --projects src/jest.config.json",
    "test:db": "WEBSOCKET_SERVER=6789 jest --projects test/database/jest.config.ts --runInBand",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --runInBand",
    "prepare": "husky install"
  },
  "dependencies": {
    "@golevelup/nestjs-webhooks": "^0.2.14",
    "@nestjs/common": "^9.1.6",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.1.6",
    "@nestjs/mapped-types": "^1.2.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.1.6",
    "@nestjs/platform-ws": "^9.1.6",
    "@nestjs/swagger": "^6.1.3",
    "@nestjs/websockets": "^9.1.6",
    "@prisma/client": "^4.5.0",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "jest-serial-runner": "^1.2.1",
    "knex": "^2.3.0",
    "lodash": "^4.17.21",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.7",
    "stripe": "^9.2.0",
    "ws": "^8.10.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@golevelup/ts-jest": "^0.3.5",
    "@nestjs/cli": "^9.1.5",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.1.6",
    "@types/bcrypt": "^5.0.0",
    "@types/express-session": "^1.17.5",
    "@types/jest": "^29.2.0",
    "@types/lodash": "^4.14.186",
    "@types/node": "^18.11.8",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.12",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "eslint": "^8.26.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "immer": "^9.0.16",
    "jest": "^29.2.2",
    "jest-extended": "^3.2.4",
    "jest-mock-extended": "^3.0.1",
    "knex-mock-client": "^1.8.4",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "prisma": "^4.5.0",
    "supertest": "^6.3.1",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.4"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "jest": {
    "projects": [
      {
        "displayName": "unit",
        "testMatch": [
          "<rootDir>/src/**/*.spec.ts"
        ],
        "transform": {
          "^.+\\.(t|j)s$": "ts-jest"
        },
        "moduleNameMapper": {
          "^src/(.*)$": "<rootDir>/src/$1"
        }
      },
      {
        "displayName": "db",
        "testMatch": [
          "<rootDir>/test/database/**.spec.ts"
        ],
        "runner": "jest-serial-runner",
        "transform": {
          "^.+\\.(t|j)s$": "ts-jest"
        },
        "moduleNameMapper": {
          "^src/(.*)$": "<rootDir>/src/$1"
        }
      }
    ],
    "rootDir": ".",
    "roots": [
      "<rootDir>/src",
      "<rootDir>/test"
    ],
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/src/$1"
    }
  },
  "prisma": {
    "schema": "./src/common/database/prisma/schema.prisma"
  }
}
